{
  "name": "your-app-name",
  "version": "1.0.0",
  "description": "Your Electron App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.yourapp.id",
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "extraFiles": [
        {
          "from": "node_modules/ffmpeg-static/bin",
          "to": ".",
          "filter": [
            "**/*"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "electron": "^30.0.1",
    "electronmon": "^2.0.3"
  }
}
